<resources>
    <!-- Base application theme. -->
    <style name="Base.Theme.Here" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Customize your light theme here. -->
        <item name="colorPrimary">@color/ic_here_background</item>
    </style>

    <style name="Theme.Here" parent="Base.Theme.Here">
        <item name="colorPrimary">@color/ic_here_background</item>
    </style>
    <style name="Theme.Here.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="colorPrimary">@color/ic_here_background</item>
    </style>
    <style name="Theme.Here.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="colorPrimary">@color/ic_here_background</item>
    </style>
    <style name="Theme.Here.PopupOverlay" parent="ThemeOverlay.AppCompat.Light">
        <item name="colorPrimary">@color/ic_here_background</item>
    </style>
</resources>